<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#BDBDBD" />
            <corners android:radius="5dp" />
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="0dp"
        android:right="1dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</layer-list>
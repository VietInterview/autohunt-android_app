<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:nestedScrollingEnabled="true">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:nestedScrollingEnabled="true"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp"
            android:nestedScrollingEnabled="true">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitlePhone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/phone_number"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvPhone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="0989421150"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="Email"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="hiep.nguyen@vietinterview.com"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleGender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/sex"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvGender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="Nam"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleAdd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:paddingHorizontal="@dimen/activity_vertical_margin"
                android:text="@string/address"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvAdd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvTitleAdd"
                android:layout_alignParentBottom="true"
                android:paddingHorizontal="@dimen/activity_vertical_margin"
                android:text="B2907, Xuân Mai Tower, Quận Hà Đông, Hà Nội"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleCity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/city"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvCity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="Hà Nội"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleSignle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/married"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvSignle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="Đã có gia đình"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitlePositionWish"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/position_wish"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvPositionWish"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="Chuyên gia"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleLevelNow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/level_current"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvLevelNow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="Quản lý"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleLevelWish"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/level_wish"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvLevelWish"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="Chuyên gia"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleCarrer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/carrer"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvCarrer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="IT - Phần mềm, CNTT, Viễn thông"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleAddWork"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/location_work"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvAddWork"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="Hà Nội"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleEducation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/level"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvEducation"
                android:layout_width="wrap_content"

                android:padding="@dimen/activity_vertical_margin"
                android:text="Đại Học"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleExp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/exp_years"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvExp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="10 năm"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleSalary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/salary_wish"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvSalary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="1,2000 USD"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="89dp">

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTitleTarget"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="@string/target"
                android:textColor="#505059"
                android:textSize="18sp" />

            <com.vietinterview.getbee.customview.NunitoTextView
                android:id="@+id/tvTarget"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/activity_vertical_margin"
                android:text="không có dữ liệu"
                android:textColor="#B1B1BA"
                android:textSize="18sp" />
        </RelativeLayout>
    </LinearLayout>
</ScrollView>
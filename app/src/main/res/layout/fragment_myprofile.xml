<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.vietinterview.getbee.customview.TouchDetectableScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:id="@+id/scrollView"
        android:layout_height="match_parent">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:context=".fragments.MyProfileFragment">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="89dp">

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvTitleName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Họ và tên"
                    android:textColor="#B1B1BA"
                    android:textSize="16sp" />

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Nguyễn Sơn Tùng"
                    android:textColor="#191830"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:src="@drawable/ic_pencil"
                    android:visibility="gone" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin"
                    android:background="#000000" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="89dp">

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvTitlePass"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Mật khẩu"
                    android:textColor="#B1B1BA"
                    android:textSize="16sp" />

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvPass"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:inputType="textPassword"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="hiepnn1991"
                    android:textColor="#191830"
                    android:textSize="20sp" />

                <ImageView
                    android:id="@+id/imgChangePass"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:src="@drawable/ic_pencil" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin"
                    android:background="#000000" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="89dp">

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvTitlePhone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Số điện thoại"
                    android:textColor="#B1B1BA"
                    android:textSize="16sp" />

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvPhone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="0989421150"
                    android:textColor="#191830"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:src="@drawable/ic_pencil"
                    android:visibility="gone" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin"
                    android:background="#000000" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="89dp">

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvTitleEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Email"
                    android:textColor="#B1B1BA"
                    android:textSize="16sp" />

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="hiep.nguyen@vietinterview.com"
                    android:textColor="#191830"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:src="@drawable/ic_pencil"
                    android:visibility="gone" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin"
                    android:background="#000000" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="89dp">

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvTitleAdd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Địa chỉ"
                    android:textColor="#B1B1BA"
                    android:textSize="16sp" />

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvAdd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Hà Nội, Việt Nam"
                    android:textColor="#191830"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:src="@drawable/ic_pencil"
                    android:visibility="gone" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin"
                    android:background="#000000" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="89dp">

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvTitleJob"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Nghề Nghiệp"
                    android:textColor="#B1B1BA"
                    android:textSize="16sp" />

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvJob"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text=""
                    android:textColor="#191830"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:src="@drawable/ic_pencil"
                    android:visibility="gone" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin"
                    android:background="#000000" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlChooseJob"
                android:layout_width="match_parent"
                android:layout_height="89dp">

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvTitleHunt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="Ngành nghề muốn cộng tác"
                    android:textColor="#B1B1BA"
                    android:textSize="16sp" />

                <com.vietinterview.getbee.customview.NunitoTextView
                    android:id="@+id/tvHunt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:text="IT, Phần mềm"
                    android:textColor="#191830"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:padding="@dimen/activity_vertical_margin"
                    android:src="@drawable/ic_arrow_right" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="@dimen/activity_vertical_margin"
                    android:layout_marginRight="@dimen/activity_vertical_margin"
                    android:background="#000000" />
            </RelativeLayout>
        </LinearLayout>
    </com.vietinterview.getbee.customview.TouchDetectableScrollView>


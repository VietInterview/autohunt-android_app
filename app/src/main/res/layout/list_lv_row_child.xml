<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textViewChild"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"
        android:textColor="@android:color/black"
        android:textSize="17dp"
        android:visibility="gone" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:layout_marginHorizontal="16dp"
        android:padding="4dp">

        <com.vietinterview.getbee.customview.NunitoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Tên bằng cấp/chứng chỉ"
            android:textColor="#505059"
            android:textSize="18sp" />

        <com.vietinterview.getbee.customview.NunitoBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="Kỹ thuật viên CNTT"
            android:textColor="#B1B1BA"
            android:textSize="18sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:layout_marginHorizontal="16dp"
        android:padding="4dp">

        <com.vietinterview.getbee.customview.NunitoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Trường/đơn vị đào tạo"
            android:textColor="#505059"
            android:textSize="18sp" />

        <com.vietinterview.getbee.customview.NunitoBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="Nhân viên thiết kế"
            android:textColor="#B1B1BA"
            android:textSize="18sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:layout_marginHorizontal="16dp"
        android:padding="4dp">

        <com.vietinterview.getbee.customview.NunitoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Thời gian"
            android:textColor="#505059"
            android:textSize="18sp" />

        <com.vietinterview.getbee.customview.NunitoBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="09/2014 - 10/2015"
            android:textColor="#B1B1BA"
            android:textSize="18sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:layout_marginHorizontal="16dp"
        android:padding="4dp">

        <com.vietinterview.getbee.customview.NunitoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Chuyên nghành"
            android:textColor="#505059"
            android:textSize="18sp" />

        <com.vietinterview.getbee.customview.NunitoBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="Đồ họa đa truyền thông"
            android:textColor="#B1B1BA"
            android:textSize="18sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:layout_marginHorizontal="16dp"
        android:padding="4dp">

        <com.vietinterview.getbee.customview.NunitoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Loại tốt nghiệp"
            android:textColor="#505059"
            android:textSize="18sp" />

        <com.vietinterview.getbee.customview.NunitoBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="Khá"
            android:textColor="#B1B1BA"
            android:textSize="18sp" />
    </RelativeLayout>
</LinearLayout>